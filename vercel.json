{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/privacy",
      "destination": "/privacy.html"
    },
    {
      "source": "/terms",
      "destination": "/terms.html"
    },
    {
      "source": "/r2-audio/:path*",
      "destination": "https://pub-7e068d8c526a459ea67ff46fe3762059.r2.dev/music/:path*"
    },
    {
      "source": "/r2-backgrounds/:path*",
      "destination": "https://pub-7e068d8c526a459ea67ff46fe3762059.r2.dev/backgrounds/:path*"
    }
  ]
}
